    },
    "era": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::era.era"
    },
    "gameHaveSpecies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::game-have-specie.game-have-specie",
